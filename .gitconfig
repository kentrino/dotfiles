[core]
  excludesfile = ~/.gitignore_global

[user]
  name = kentrino
  email = kentrino@h-kento.jp
  # signingKey = A3A60735004D91924639BE45275556AD18955D2E
  signingKey = 44206B654BD9AA29AF701605CA4AC40F96EE883B

[credential]
  helper = cache --timeout=8640000

[push]
  default = current

[commit]
  gpgSign = true

[gpg]
  program = gpg

[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[init]
  defaultBranch = master

[pull]
  ff = only
